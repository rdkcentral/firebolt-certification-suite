{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true,
    "ecmaVersion": 11
  },
  "plugins": ["prettier"],
  "rules": {
    "eqeqeq":"warn",
    "func-names": "off",
    "no-case-declarations": "off",
    "no-console": "off",
    "no-eval":"error",
    "no-prototype-builtins": "off",
    "no-undef": "off",
    "no-unused-vars": "warn",
    "no-var": "error",
    "prefer-const": "error",
    "prettier/prettier": "warn",
    "spaced-comment":"error"
  }
}