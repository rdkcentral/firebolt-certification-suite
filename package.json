{
  "name": "firebolt-certification-suite",
  "version": "0.1.0",
  "description": "Firebolt Certification Test Suite",
  "main": "index.js",
  "scripts": {
    "cy:run": "node ./Scripts/checkDefaultConfig.js && sh ./run.sh run",
    "setup": "cp -r ./node_modules/configModule/constants/config.json ./supportConfig.json",
    "softRefresh": "yarn install && yarn upgrade",
    "hardRefresh": "rm -rf node_modules && yarn cache clean && yarn softRefresh",
    "format": "prettier --write '**/*.js'",
    "lint": "eslint .",
    "fixtureSetup": "bash ./Scripts/fixtureSetup.sh",
    "applyDependencyOverrides": "node ./Scripts/applyDependencyOverrides.js",
    "postinstall": "npm run setup && npm run fixtureSetup && npm run applyDependencyOverrides",
    "test": "jest --config cypress/cypress-support/test/jest.config.js"
  },
  "keywords": [],
  "author": "Comcast Firebolt Certification Team",
  "license": "ISC",
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^9.0.9",
    "@firebolt-js/manage-sdk": "1.0.0",
    "@firebolt-js/sdk": "1.0.0",
    "jsonpath": "^1.1.1",
    "jsonschema": "^1.4.0",
    "modularTransportClient": "git+ssh://git@github.com-ghec:comcast-fireboltCertification/modularTransportClient.git",
    "uuid": "^8.3.1",
    "@badeball/cypress-cucumber-preprocessor": "^15.0.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.0",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "axios": "^1.3.4",
    "configModule": "git+ssh://git@github.com-ghec:comcast-fireboltCertification/ripple-remote-config-module",
    "cucumber-json-report-formatter": "^0.1.4",
    "cypress": "^12.0.0",
    "esbuild": "^0.17.0",
    "json-merger": "1.1.10",
    "lodash": "^4.17.21",
    "mochawesome": "^6.1.1",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0",
    "multiple-cucumber-html-reporter": "^2.0.0",
    "shell-exec": "1.0.2"
  },
  "devDependencies": {
    "babel-jest": "^27.4.5",
    "jest": "^27.4.5",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.2"
  },
  "cypress-cucumber-preprocessor": {
    "json": {
      "enabled": true
    }
  }
}
