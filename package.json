{
  "name": "firebolt-certification-suite",
  "version": "0.1.0",
  "description": "Firebolt Certification Test Suite",
  "main": "index.js",
  "scripts": {
    "cy:open": "node ./Scripts/checkDefaultConfig.js && yarn cache clean && sh ./Scripts/run.sh open",
    "cy:run": "node ./Scripts/checkDefaultConfig.js && yarn cache clean && bash ./Scripts/run.sh run",
    "softRefresh": "yarn install && yarn upgrade",
    "hardRefresh": "rm -rf node_modules && yarn cache clean && yarn softRefresh",
    "copyReportTemplate": "node ./Scripts/copyReportTemplate.js",
    "copyExternalConfigData": "node ./Scripts/copyExternalConfigData.js",
    "applyDependencyOverrides": "node ./Scripts/applyDependencyOverrides.js",
    "setup": "npm run copyExternalConfigData && npm run applyDependencyOverrides && npm run copyCukeReportTemplate",
    "postinstall": "npm run setup",
    "format": "prettier --write \"**/*.js\"",
    "lint": "eslint .",
    "test": "jest --config ./cypress/support/cypress-support/test/jest.config.js"
  },
  "keywords": [],
  "author": "Comcast Firebolt Certification Team",
  "license": "ISC",
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^9.0.9",
    "@badeball/cypress-cucumber-preprocessor": "^15.0.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.0",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@firebolt-js/manage-sdk": "1.0.0",
    "@firebolt-js/sdk": "1.0.0",
    "axios": "^1.3.4",
    "configModule": "./defaultModule",
    "cucumber-json-report-formatter": "^0.1.4",
    "cypress": "^12.0.0",
    "esbuild": "^0.17.0",
    "jsdom": "^24.0.0",
    "json-merger": "1.1.10",
    "jsonpath": "^1.1.1",
    "jsonschema": "^1.4.0",
    "lint": "^0.8.19",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "lzutf8": "^0.6.3",
    "mochawesome": "^6.1.1",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0",
    "multiple-cucumber-html-reporter": "^2.0.0",
    "setimmediate": "^1.0.5",
    "shell-exec": "1.0.2",
    "uuid": "^8.3.1",
    "winston": "3.8.2"
  },
  "devDependencies": {
    "babel-jest": "^27.4.5",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^27.4.5",
    "prettier": "^3.0.2"
  },
  "cypress-cucumber-preprocessor": {
    "json": {
      "enabled": true
    }
  }
}
